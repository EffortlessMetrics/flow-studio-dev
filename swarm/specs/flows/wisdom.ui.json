{
  "$schema": "../schemas/flow_ui_overlay.schema.json",
  "flow_id": "wisdom",
  "version": "1.0.0",
  "nodes": {
    "audit": {
      "position": { "x": 100, "y": 200 },
      "color": "node_highlight",
      "collapsed": false,
      "pinned": true
    },
    "regression": {
      "position": { "x": 300, "y": 200 },
      "color": "node_highlight",
      "collapsed": false,
      "pinned": true
    },
    "history": {
      "position": { "x": 500, "y": 200 },
      "color": "node_highlight",
      "collapsed": false,
      "pinned": true
    },
    "learnings": {
      "position": { "x": 700, "y": 200 },
      "color": "node_highlight",
      "collapsed": false,
      "pinned": true
    },
    "feedback": {
      "position": { "x": 900, "y": 200 },
      "color": "node_highlight",
      "collapsed": false,
      "pinned": true
    },
    "wisdom_report": {
      "position": { "x": 1100, "y": 200 },
      "color": "node_highlight",
      "collapsed": false,
      "pinned": true
    }
  },
  "edges": {},
  "palette": {
    "node_default": "#4a5568",
    "node_highlight": "#9f7aea",
    "node_active": "#b794f4",
    "node_completed": "#2d3748",
    "node_error": "#e53e3e",
    "edge_default": "#718096",
    "edge_loop": "#ed8936",
    "edge_branch": "#4299e1",
    "background": "#1a202c",
    "grid": "#2d3748"
  },
  "canvas": {
    "zoom": 1,
    "pan": { "x": 0, "y": 0 },
    "grid_size": 20,
    "grid_visible": true,
    "snap_to_grid": true
  },
  "groups": [
    {
      "id": "analysis",
      "label": "SDLC Analysis",
      "node_ids": ["audit", "regression", "history"],
      "color": "#9f7aea80",
      "collapsed": false
    },
    {
      "id": "improvement",
      "label": "Continuous Improvement",
      "node_ids": ["learnings", "feedback"],
      "color": "#805ad580",
      "collapsed": false
    }
  ],
  "annotations": [
    {
      "id": "closed_loop_note",
      "text": "CLOSED LOOP: Learnings feed back to Flows 1-3",
      "position": { "x": 700, "y": 100 },
      "style": {
        "font_size": 12,
        "color": "#b794f4"
      }
    },
    {
      "id": "complete_note",
      "text": "SDLC cycle complete",
      "position": { "x": 1100, "y": 280 },
      "style": {
        "font_size": 14,
        "color": "#9f7aea"
      }
    }
  ]
}
